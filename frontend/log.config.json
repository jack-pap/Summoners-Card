{
    "recurseDepth": 10,
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.(js|jsx)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "./node_modules/docdash",
        "destination": "./docs",
        "recurse": true,
        "readme": "../README.md"
    },
    "metadata": { "title": "Summoners Card" },
    "docdash": {
        "static": true,
        "sort": true,
		"meta": {
			"title": "Summoners Card",
			"description": "A stats viewing fullstack web application using the Riot API",
			"keyword": "jsdoc, docdash"
		},
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu":{
            "Github Repository":{
                "href":"https://github.com/jack-pap/Summoners-Card",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        }
    }
}
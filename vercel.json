{
    "version": 2,
    "builds": [
        {
            "src": "src/app/api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/src/api/index.js"
        }
    ]
}
{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        },
        {
            "src": "src/app/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/proxy/(.*)",
            "dest": "/api/index.js"
        }
    ]
}